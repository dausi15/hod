@prefix bf: <http://buildsys.org/ontologies/BrickFrame#> .
@prefix brick: <http://buildsys.org/ontologies/Brick#> .
@prefix btag: <http://buildsys.org/ontologies/BrickTag#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soda_hall: <http://buildsys.org/ontologies/building_example#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

soda_hall:ahu_1 a brick:AHU ;
    bf:feeds soda_hall:vav_1 .

soda_hall:room_1 a brick:Room ;
    bf:isPartOf soda_hall:floor_1,
        soda_hall:hvaczone_1 .

soda_hall:ztemp_1 a brick:Zone_Temperature_Sensor ;
    bf:isPointOf soda_hall:vav_1 .

soda_hall:floor_1 a brick:Floor .

soda_hall:hvaczone_1 a brick:HVAC_Zone .

soda_hall:vav_1 a brick:VAV ;
    bf:feeds soda_hall:hvaczone_1 .

